<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <display-name></display-name>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>setSession</servlet-name>
    <servlet-class>servlet.setSession</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>ListUser</servlet-name>
    <servlet-class>find.ListUser</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>setSession</servlet-name>
    <url-pattern>/servlet/setSession</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ListUser</servlet-name>
    <url-pattern>/servlet/ListUser</url-pattern>
  </servlet-mapping>
</web-app>